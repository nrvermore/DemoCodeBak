package com.labwinner.domain;

import java.sql.Timestamp;
import java.util.Date;

/**
 * 信息广场
 * @author xux
 *
 */
public class MarketAssistVoice implements java.io.Serializable{
	
	/**
	 * 信息广场主键
	 */
	private Integer marketAssistVoiceId;
	
	/**
	 * 信息广场
	 */
	private MarketAssist marketAssist;
	
	/**
	 * 语音发送人
	 */
	private SysUser sysUser;
	
	/**
	 * 信息广场语音名称
	 */
	private String marketAssistVoiceName;
	
	
	/**
	 * 信息广场语音发送时间
	 */
	private Date createDate;

	public MarketAssistVoice() {
		super();
		// TODO Auto-generated constructor stub
	}

	public MarketAssistVoice(Integer marketAssistVoiceId,
			MarketAssist marketAssist, SysUser sysUser,
			String marketAssistVoiceName, Timestamp createDate) {
		super();
		this.marketAssistVoiceId = marketAssistVoiceId;
		this.marketAssist = marketAssist;
		this.sysUser = sysUser;
		this.marketAssistVoiceName = marketAssistVoiceName;
		this.createDate = createDate;
	}

	public Integer getMarketAssistVoiceId() {
		return marketAssistVoiceId;
	}

	public void setMarketAssistVoiceId(Integer marketAssistVoiceId) {
		this.marketAssistVoiceId = marketAssistVoiceId;
	}

	public MarketAssist getMarketAssist() {
		return marketAssist;
	}

	public void setMarketAssist(MarketAssist marketAssist) {
		this.marketAssist = marketAssist;
	}

	public SysUser getSysUser() {
		return sysUser;
	}

	public void setSysUser(SysUser sysUser) {
		this.sysUser = sysUser;
	}

	public String getMarketAssistVoiceName() {
		return marketAssistVoiceName;
	}

	public void setMarketAssistVoiceName(String marketAssistVoiceName) {
		this.marketAssistVoiceName = marketAssistVoiceName;
	}

	public Date getCreateDate() {
		return createDate;
	}

	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}
	
}
