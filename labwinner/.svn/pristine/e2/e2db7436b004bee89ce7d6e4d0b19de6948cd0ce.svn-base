package com.labwinner.domain;

import java.util.HashSet;
import java.util.Set;

public class ModifyCode {
	
	private  Integer modifyCodeId;
	
	private String modifyCode;

	/**
	 * 是否是基础变更原因  0不是  1 是
	 */
	private Integer flag;
	
	private Set<InventoryModify> inventoryModifies = new HashSet<InventoryModify>(
			0);

	public ModifyCode() {
		super();
		// TODO Auto-generated constructor stub
	}

	public ModifyCode(Integer modifyCodeId, String modifyCode, Integer flag,
			Set<InventoryModify> inventoryModifies) {
		super();
		this.modifyCodeId = modifyCodeId;
		this.modifyCode = modifyCode;
		this.flag = flag;
		this.inventoryModifies = inventoryModifies;
	}

	public Integer getModifyCodeId() {
		return modifyCodeId;
	}

	public void setModifyCodeId(Integer modifyCodeId) {
		this.modifyCodeId = modifyCodeId;
	}

	public String getModifyCode() {
		return modifyCode;
	}

	public void setModifyCode(String modifyCode) {
		this.modifyCode = modifyCode;
	}

	public Integer getFlag() {
		return flag;
	}

	public void setFlag(Integer flag) {
		this.flag = flag;
	}

	public Set<InventoryModify> getInventoryModifies() {
		return inventoryModifies;
	}

	public void setInventoryModifies(Set<InventoryModify> inventoryModifies) {
		this.inventoryModifies = inventoryModifies;
	}
	
	
	
}
